{
  "name": "@will-stone/eslint-config",
  "version": "16.0.1",
  "description": "Will Stone's ESLint config",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/will-stone/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/will-stone/eslint-config.git"
  },
  "license": "MIT",
  "author": "Will Stone",
  "type": "module",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts --shims",
    "predev": "pnpm build",
    "dev": "tsup src/index.ts --format esm --shims --watch & config-inspector --config ./eslint-config-for-inspector.js",
    "lint": "pnpm stub && eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "release": "release-it",
    "stub": "tsup src/index.ts --format esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-import-x": "^4.10.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-package-json": "^0.29.1",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "type-fest": "^4.40.0"
  },
  "devDependencies": {
    "@commits-with-character/conventional-changelog-preset": "^4.0.0",
    "@eslint/config-inspector": "^1.0.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.14.1",
    "@typescript-eslint/utils": "^8.30.1",
    "@vitest/eslint-plugin": "^1.1.43",
    "@will-stone/prettier-config": "^11.0.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "memfs": "^4.17.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@vitest/eslint-plugin": "^1.1.43",
    "astro-eslint-parser": "^1.2.2",
    "eslint": ">=9.23.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.0"
  },
  "peerDependenciesMeta": {
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.6.5",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
