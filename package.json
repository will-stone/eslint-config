{
  "name": "@will-stone/eslint-config",
  "version": "10.0.1",
  "description": "Will Stone's ESLint config",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/will-stone/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/will-stone/eslint-config.git"
  },
  "license": "MIT",
  "author": "Will Stone",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts --cjsInterop --splitting",
    "dev": "tsup src/index.ts --format esm,cjs --watch & config-inspector",
    "lint": "npm run stub && eslint .",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "stub": "tsup src/index.ts --format esm",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "prettier": "@will-stone/prettier-config",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "astro-eslint-parser": "^1.0.2",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.10.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "glob": "^11.0.0",
    "globals": "^15.9.0"
  },
  "devDependencies": {
    "@commits-with-character/conventional-changelog-preset": "^0.2.2",
    "@eslint/config-inspector": "^0.5.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.1.0",
    "@will-stone/prettier-config": "^8.0.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "tsup": "^8.2.4",
    "type-fest": "^4.23.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": ">=9.8.0"
  },
  "overrides": {
    "eslint": ">=9.8.0"
  }
}
