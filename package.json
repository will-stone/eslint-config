{
  "name": "@will-stone/eslint-config",
  "version": "25.2.1",
  "description": "Will Stone's ESLint config",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/will-stone/eslint-config#readme",
  "bugs": "https://github.com/will-stone/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/will-stone/eslint-config.git"
  },
  "license": "MIT",
  "author": "Will Stone",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "config-inspector --config ./eslint-config-for-inspector.js",
    "lint": "eslint . --concurrency=auto",
    "lint:debug": "DEBUG=ws eslint . --concurrency=auto",
    "prepare": "husky",
    "release": "release-it",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.8.0",
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/debug": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "confusing-browser-globals": "^1.0.11",
    "debug": "^4.4.3",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-package-json": "^0.88.2",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "globals": "^17.3.0",
    "globby": "^16.1.0",
    "type-fest": "^5.4.4",
    "typescript-eslint": "^8.55.0"
  },
  "devDependencies": {
    "@commits-with-character/conventional-changelog-preset": "^5.1.1",
